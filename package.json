{
  "name": "cms-prototype",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "workers:dev": "wrangler dev",
    "workers:build": "tsc --project tsconfig.workers.json",
    "workers:deploy": "wrangler deploy",
    "workers:tail": "wrangler tail",
    "db:create": "wrangler d1 create cms-prototype-db",
    "db:local": "wrangler d1 execute cms-prototype-db --local --file=migrations/d1-schema.sql && wrangler d1 execute cms-prototype-db --local --file=migrations/d1-seed.sql",
    "db:remote": "wrangler d1 execute cms-prototype-db --file=migrations/d1-schema.sql && wrangler d1 execute cms-prototype-db --file=migrations/d1-seed.sql",
    "db:migrations": "wrangler d1 execute cms-prototype-db --file=migrations/d1-schema.sql"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.2",
    "@tiptap/extension-color": "^3.19.0",
    "@tiptap/extension-placeholder": "^3.19.0",
    "@tiptap/extension-text-style": "^3.19.0",
    "@tiptap/pm": "^3.19.0",
    "@tiptap/react": "^3.19.0",
    "@tiptap/starter-kit": "^3.19.0",
    "bcrypt": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.3.1",
    "lucide-react": "^0.564.0",
    "mysql2": "^3.17.1",
    "next": "16.1.6",
    "next-auth": "^4.24.13",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.71.1",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260214.0",
    "@tailwindcss/postcss": "^4",
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wrangler": "^4.65.0"
  }
}
